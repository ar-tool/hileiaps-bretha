<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">

    <!-- 角度 -->
    <corners android:radius="6dp"/>
    <!-- 填充色 -->
    <solid android:color="#ffffff"/>
    <!-- 描边 设置线宽及颜色 -->
    <stroke android:color="#cccacb"
        android:width="1dp"/>
    <padding
        android:left="5dp"
        android:top="5dp"
        android:right="5dp"
        android:bottom="5dp"/>
</shape>